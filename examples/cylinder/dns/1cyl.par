[GENERAL]
startFrom = rstcyl0.f00001

stopAt = endTime
endTime = 1

userParam01 = 0 # DNS mode

dt = 0
variableDt = yes
timeStepper = bdf3
targetCFL = 0.5

writeControl = runTime
writeInterval = 100.
writeDoublePrecision = no

dealiasing = yes
#filtering = explicit
#filterWeight = 0.01
#filterCutoffRatio = 0.84

[PRESSURE]
preconditioner = semg_xxt
residualTol = 1.0E-05
residualProj = yes
writeToFieldFile = yes

[VELOCITY]
residualTol = 1.0E-07
residualProj = no
density = 1.0
viscosity = -50.0
